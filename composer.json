{
    "name": "lynnuniversity/lynnworkflow",
    "description": "Workflow solution for Lynn University",
    "type": "craft-plugin",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "lynn workflow"
    ],
    "support": {
        "docs": "https://github.com/happycog/workflow/blob/master/README.md"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Lynn University",
            "homepage": "https://www.lynn.edu"
        }
    ],
    "require": {
        "craftcms/cms": "^3.2.0"
    },
    "autoload": {
        "psr-4": {
          "therefinery\\lynnworkflow\\": "src/"
        }
    },
    "extra": {
        "name": "LynnWorkflow",
        "handle": "lynnworkflow",
        "schemaVersion": "2.0.0",
        "hasCpSettings": true,
        "hasCpSection": true,
        "changelogUrl": "https://github.com/happycog/workflow/blob/master/CHANGELOG.md",
        "class": "therefinery\\lynnworkflow\\LynnWorkflow"
    }
}
